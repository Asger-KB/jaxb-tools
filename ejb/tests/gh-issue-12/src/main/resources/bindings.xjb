<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
	version="2.1"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:hj="http://hyperjaxb3.jvnet.org/ejb/schemas/customizations"
	xmlns:orm="http://java.sun.com/xml/ns/persistence/orm"
	jaxb:extensionBindingPrefixes="hj orm">

	<jaxb:bindings schemaLocation="schema.xsd" node="/xs:schema">
		<jaxb:globalBindings generateIsSetMethod="true"/>
		<jaxb:schemaBindings>
			<jaxb:package name="org.jvnet.hyperjaxb3.ejb.tests.gh_issue_12"/>
		</jaxb:schemaBindings>
		<jaxb:bindings node="xs:complexType[@name='InfrastructureRoot.typeId']">
			<hj:entity>
				<orm:table name="typeId"/>
			</hj:entity>
		</jaxb:bindings>
		<jaxb:bindings node="xs:complexType[@name='II']">
			<hj:entity>
				<orm:table name="II"/>
			</hj:entity>
		</jaxb:bindings>
		<jaxb:bindings node="xs:complexType[@name='ANY']">
			<hj:entity>
				<orm:table name="any"/>
			</hj:entity>
		</jaxb:bindings>
	</jaxb:bindings>


</jaxb:bindings>
